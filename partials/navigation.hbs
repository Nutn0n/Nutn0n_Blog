<button class="oak-navigation-button" style="display: inline-block;
border-radius: 2px;
padding: 5px 7px 5px;
margin-left: 5px;
box-shadow: 0 0 0 1px #333;">MENU</button>
<nav class="oak-navigation" role="navigation">
	<ul>
		{{! Place for your navigation links}}

		{{! Navigation links for built in ghost navigation (for Ghost version 0.5.9  or later)}}
		{{#if @blog.navigation}}
		{{#foreach navigation}}
		<li class="nav-{{slug}}{{#if current}} nav-current{{/if}}" role="presentation">
			<a class="oak-uppercase" href="{{url}}">{{label}}</a>
		</li>
		{{/foreach}}
		{{/if}}

		{{! Bellow here you can manualy add navigation links (for Ghost versions 0.5.8 or prior) }}

	</ul>
</nav>
